<% if other.count == 1 && other.first.missing? %>
  <%= render 'facets/results', facet: facet, visible: visible + other, other: [], link: method(:link_to_terms_facet)  %>
<% else %>
  <%= render 'facets/results', facet: facet, visible: visible, other: other, link: method(:link_to_terms_facet)  %>
<% end %>
